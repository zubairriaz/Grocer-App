{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "grocer-app": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/grocer-app",
      "sourceRoot": "apps/grocer-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/grocer-app",
            "index": "apps/grocer-app/src/index.html",
            "main": "apps/grocer-app/src/main.ts",
            "polyfills": "apps/grocer-app/src/polyfills.ts",
            "tsConfig": "apps/grocer-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/grocer-app/src/favicon.ico",
              "apps/grocer-app/src/assets",
              "apps/grocer-app/src/android-chrome-192x192.png",
              "apps/grocer-app/src/favicon-16x16.png",
              "apps/grocer-app/src/mstile-310x150.png",
              "apps/grocer-app/src/android-chrome-512x512.png",
              "apps/grocer-app/src/favicon-32x32.png",
              "apps/grocer-app/src/mstile-310x310.png",
              "apps/grocer-app/src/apple-touch-icon.png",
              "apps/grocer-app/src/manifest.json",
              "apps/grocer-app/src/mstile-70x70.png",
              "apps/grocer-app/src/browserconfig.xml",
              "apps/grocer-app/src/mstile-144x144.png",
              "apps/grocer-app/src/safari-pinned-tab.svg",
              "apps/grocer-app/src/mstile-150x150.png"
            ],
            "styles": [
              "apps/grocer-app/src/grocer-app-theme.scss",
              "apps/grocer-app/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/grocer-app/src/environments/environment.ts",
                  "with": "apps/grocer-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "grocer-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "grocer-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "grocer-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/grocer-app/src/test.ts",
            "polyfills": "apps/grocer-app/src/polyfills.ts",
            "tsConfig": "apps/grocer-app/tsconfig.spec.json",
            "karmaConfig": "apps/grocer-app/karma.conf.js",
            "assets": ["apps/grocer-app/src/favicon.ico", "apps/grocer-app/src/assets"],
            "styles": ["apps/grocer-app/src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/grocer-app/tsconfig.app.json",
              "apps/grocer-app/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "grocer-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "grocer-app:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    },
    "grocer-app-e2e": {
      "root": "apps/grocer-app-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/grocer-app-e2e/protractor.conf.js",
            "devServerTarget": "grocer-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "grocer-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/grocer-app-e2e/tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "server": {
      "root": "apps/server",
      "sourceRoot": "apps/server/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/server",
            "main": "apps/server/src/main.ts",
            "tsConfig": "apps/server/tsconfig.app.json",
            "assets": ["apps/server/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/server/src/environments/environment.ts",
                  "with": "apps/server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "server:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/server/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/server"],
          "options": {
            "jestConfig": "apps/server/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "defaultProject": "grocer-app",
  "cli": {
    "defaultCollection": "@nrwl/angular"
  }
}
